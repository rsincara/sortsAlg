<main>
    <h1>Быстрая сортировка</h1>
    <p>Сложность данного алгоритма - O(N*Log(N))</p>
    <form method="get" action="~/Home/QsortM">
        <label>
            <textarea placeholder="Введите текст"  rows="5" cols="45"></textarea>
        </label>
        <br/>
        <input type="submit" value="Получить результат" class="btn">
    </form>
    
      <div class="result">
            @{
                foreach (var pair in TextModel.Words)
                {
                    <p>@pair.Key встретилось @pair.Value раз</p>
                }
            }
        </div>
<div class="authors">
        <p>ПрИ-201</p>
        <p>Ермаков Максим</p>
        <p>Григоричев Александр</p>
    </div>
</main>
